{
    "version": 3,
    "builds": [
      {
        "src": "vercel_app/wsgi.py",
        "use": "@vercel/python-wsgi",
        "config": {
          "maxLambdaSize": "150mb",
          "python": "3.12"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "vercel_app/wsgi.py"
      }
    ]
  }
  